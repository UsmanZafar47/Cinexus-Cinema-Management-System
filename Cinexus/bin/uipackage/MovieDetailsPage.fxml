<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="800.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MovieDetailsPage">
   <children>
      <Label prefHeight="28.0" prefWidth="118.0" style="-fx-background-color: red; -fx-text-fill: white; -fx-font-size: 19;" text="Movie Details">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
      <Label fx:id="movieNameLabel" prefHeight="17.0" prefWidth="341.0" style="-fx-background-color: black; -fx-text-fill: white;" stylesheets="@designLayout.css" text="Movie Name:    ">
         <VBox.margin>
            <Insets left="40.0" />
         </VBox.margin>
         <padding>
            <Insets top="15.0" />
         </padding></Label>
      <Label fx:id="cinemaNameLabel" prefHeight="74.0" prefWidth="342.0" style="-fx-background-color: black; -fx-text-fill: white;" text="Cinema Name: ">
         <VBox.margin>
            <Insets left="40.0" />
         </VBox.margin></Label>
      <Label fx:id="showtimesLabel" prefHeight="96.0" prefWidth="342.0" style="-fx-background-color: black; -fx-text-fill: white;" text="Showtimes:">
         <VBox.margin>
            <Insets left="40.0" />
         </VBox.margin></Label>
   </children>
</VBox>
